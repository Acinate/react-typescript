{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "strictNullChecks": false,
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "es2015",
    "outDir": "dist",
    "jsx": "react"
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
